    "bootstrap": "^3.3.6",
    "debug": "^2.2.0",
    "deep-extend": "^0.4.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^0.7.7",
    "leaflet-draw": "^0.3.0",
    "leaflet-transit-editor": "^0.0.9",
    "react": "^15.0.1",
    "react-dock": "^0.2.3",
    "react-dom": "^15.0.1",
