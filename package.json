{
  "name": "scenario-editor",
  "version": "0.0.1",
  "description": "conveyal's tools for editing land use/transportation scenarios.",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "standard": {
