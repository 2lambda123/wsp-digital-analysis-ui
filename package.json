{
  "name": "@conveyal/analysis-ui",
  "version": "0.0.0-semantic.release",
  "description": "Conveyal's tool for editing land use/transportation scenarios.",
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "build": "mastarm build",
    "deploy": "mastarm deploy --config configurations/default",
    "docs": "cd docs && mkdocs serve",
    "jest": "mastarm test --env test --config configurations/test",
    "lint": "mastarm lint lib/",
    "predeploy": "yarn",
    "pretest": "yarn",
    "prestart": "yarn",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "mastarm build --serve",
    "test": "mastarm lint lib/ && mastarm test --env test --config configurations/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/analysis-ui.git"
  },
  "license": "MIT",
  "bugs": {
