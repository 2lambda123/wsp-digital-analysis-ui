  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "mastarm serve lib/index.js --proxy http://localhost:7070/api",
    "docs": "cd docs && mkdocs serve",
    "deploy": "mastarm deploy --config configurations/default"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conveyal/scenario-editor.git"
