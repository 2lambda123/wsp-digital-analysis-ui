{
  "name": "analysis",
  "scope": "conveyal",
  "alias": "next.conveyal.com",
  "public": true,
  "version": 2,
  "regions": ["all"],
  "builds": [
    {"src": "next.config.js", "use": "@now/next"},
    {"src": "api/proxy/index.js", "use": "@now/node"}
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy/index.js"
    }
  ],
  "env": {
    "API_URL": "@api-url"
  },
  "build": {
    "env": {
      "API_URL": "@api-url",
      "AUTH0_CLIENT_ID": "@auth0-client-id",
      "AUTH0_DOMAIN": "@auth0-domain",
      "MAPBOX_ACCESS_TOKEN": "@mapbox-access-token"
    }
  }
}
