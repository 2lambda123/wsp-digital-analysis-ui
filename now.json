{
  "name": "analysis",
  "scope": "conveyal",
  "public": true,
  "version": 2,
  "builds": [
    {"src": "next.config.js", "use": "@now/next"},
    {"src": "api/proxy/index.js", "use": "@now/node"}
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy/index.js"
    }
  ],
  "env": {
    "API_URL": "@api-url"
  },
  "build": {
    "env": {
      "AUTH0_CLIENT_ID": "@auth0-client-id",
      "AUTH0_DOMAIN": "@auth0-domain",
      "GRAPHHOPPER_API_KEY": "@graphhopper-api-key",
      "MAPTILER_KEY": "@maptiler-key"
    }
  }
}
